package aaa;

public class MethodOverriding {

	public static void main(String[] args) {

	A obj = new A();
	B obj1 = new B();	
	}

}

class A {

	public A() 
	{
		System.out.println("method A without parameter");
	}
}

class B extends A
{

	public B() {
		System.out.println("method B without parameter");
	}
}
