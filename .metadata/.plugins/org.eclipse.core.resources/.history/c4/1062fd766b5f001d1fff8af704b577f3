package revisionAll;

public class ReverseStringCharacter {

	public static void main(String[] args) {
		
		String s1, s2;
		s1="Hello";
		s2="World";
		
		s1= s1+s2;
		
		s2= s1.substring(0, s1.length()-s2.length());
		
		s1= s1.substring(s1.length());
		
		System.out.println("swapping two string s1"+s1);
		System.out.println("swapping two string s2"+s2);

	}

}
