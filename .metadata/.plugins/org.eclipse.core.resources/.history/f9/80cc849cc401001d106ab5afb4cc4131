package aaa;

public class DemoException {

	public static void main(String[] args) throws ArithmeticException
	{
		
        int i,j,k=0;
        i=9;
        j=0;
        try {
        k=i/j;
        }
        catch(Exception e)
        {
        System.out.println(e);
        }
        System.out.println(k);

	}

}
