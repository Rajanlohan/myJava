package aaa;

public class DemoException 
{

	public static void main(String[] args) 
	{
		
        int i,j,k=0;
        i=9;
        j=0;
        int a[] = new int[4];
        try 
        {
        k=i/j;
        for(int m=0; m<=4; m++)
        {
        	a[m] = m+1;
        }
        }
        catch(ArithmeticException e)
        {
        System.out.println(e);
        }
       
        System.out.println(k);

	}

}
